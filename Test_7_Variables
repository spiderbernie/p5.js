function setup() {
  createCanvas(720,400);
  background(0);
  stroke(153);
  strokeWeight(4);
  strokeCap(SQUARE);
  
  let a = 50;
  let b = 120;
  let c = 180;
  
var y = 2*(a+c);
for(a; a<y; a=a+c) {
  if(a==a+c) {
      let x = 10;
  let i;
    for(i=0; i<(4*x); i += x)
  
  line(a,height-b+i,a+c,height-b+i);
  }
  else {
  let x = 10;
  let i;
    for(i=0; i<(4*x); i += x)
  
  line(a,b+i,a+c,b+i);
}
}
}
