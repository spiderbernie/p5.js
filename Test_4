//Estudio de coordenadas

function setup() {
  createCanvas(400, 400);
}

function draw() {
  background(220);
  
  stroke(50,50,50);
  noFill(0);
  rect(100,100,20,20);
  
  fill(250);
  noStroke();
  rect(300,100,-20,20);
  
  stroke(250); 
  noFill();
  rect(100,300,20,-20);
  
  point(width/2,width/2);
    
  stroke(0);
  line(0,0,width,height);
  
  stroke(250)
  line(width,0,0,height);
  
  ellipse(width/2,height/2,100,100)
 
  noStroke()
  fill(0)
  rect(width-100,height-100,-20,-20)
}
